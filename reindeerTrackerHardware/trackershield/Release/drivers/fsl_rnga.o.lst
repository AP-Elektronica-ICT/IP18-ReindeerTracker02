  58:../drivers/fsl_rnga.c **** #define RNG_RD_CR(base) (RNG_CR_REG(base))
  59:../drivers/fsl_rnga.c **** #define RNG_WR_CR(base, value) (RNG_CR_REG(base) = (value))
  60:../drivers/fsl_rnga.c **** #define RNG_RMW_CR(base, mask, value) (RNG_WR_CR(base, (RNG_RD_CR(base) & ~(mask)) | (value)))
  61:../drivers/fsl_rnga.c **** /*@}*/
  62:../drivers/fsl_rnga.c **** 
  63:../drivers/fsl_rnga.c **** /*!
  64:../drivers/fsl_rnga.c ****  * @name Register RNG_CR, field GO[0] (RW)
  65:../drivers/fsl_rnga.c ****  *
  66:../drivers/fsl_rnga.c ****  * Specifies whether random-data generation and loading (into OR[RANDOUT]) is
  67:../drivers/fsl_rnga.c ****  * enabled.This field is sticky. You must reset RNGA to stop RNGA from loading
  68:../drivers/fsl_rnga.c ****  * OR[RANDOUT] with data.
  69:../drivers/fsl_rnga.c ****  *
  70:../drivers/fsl_rnga.c ****  * Values:
  71:../drivers/fsl_rnga.c ****  * - 0b0 - Disabled
  72:../drivers/fsl_rnga.c ****  * - 0b1 - Enabled
  73:../drivers/fsl_rnga.c ****  */
  74:../drivers/fsl_rnga.c **** /*@{*/
  75:../drivers/fsl_rnga.c **** /*! @brief Read current value of the RNG_CR_GO field. */
  76:../drivers/fsl_rnga.c **** #define RNG_RD_CR_GO(base) ((RNG_CR_REG(base) & RNG_CR_GO_MASK) >> RNG_CR_GO_SHIFT)
  77:../drivers/fsl_rnga.c **** 
  78:../drivers/fsl_rnga.c **** /*! @brief Set the GO field to a new value. */
  79:../drivers/fsl_rnga.c **** #define RNG_WR_CR_GO(base, value) (RNG_RMW_CR(base, RNG_CR_GO_MASK, RNG_CR_GO(value)))
  80:../drivers/fsl_rnga.c **** /*@}*/
  81:../drivers/fsl_rnga.c **** 
  82:../drivers/fsl_rnga.c **** /*!
  83:../drivers/fsl_rnga.c ****  * @name Register RNG_CR, field SLP[4] (RW)
  84:../drivers/fsl_rnga.c ****  *
  85:../drivers/fsl_rnga.c ****  * Specifies whether RNGA is in Sleep or Normal mode. You can also enter Sleep
  86:../drivers/fsl_rnga.c ****  * mode by asserting the DOZE signal.
  87:../drivers/fsl_rnga.c ****  *
  88:../drivers/fsl_rnga.c ****  * Values:
  89:../drivers/fsl_rnga.c ****  * - 0b0 - Normal mode
  90:../drivers/fsl_rnga.c ****  * - 0b1 - Sleep (low-power) mode
  91:../drivers/fsl_rnga.c ****  */
  92:../drivers/fsl_rnga.c **** /*@{*/
  93:../drivers/fsl_rnga.c **** /*! @brief Read current value of the RNG_CR_SLP field. */
  94:../drivers/fsl_rnga.c **** #define RNG_RD_CR_SLP(base) ((RNG_CR_REG(base) & RNG_CR_SLP_MASK) >> RNG_CR_SLP_SHIFT)
  95:../drivers/fsl_rnga.c **** 
  96:../drivers/fsl_rnga.c **** /*! @brief Set the SLP field to a new value. */
  97:../drivers/fsl_rnga.c **** #define RNG_WR_CR_SLP(base, value) (RNG_RMW_CR(base, RNG_CR_SLP_MASK, RNG_CR_SLP(value)))
  98:../drivers/fsl_rnga.c **** /*@}*/
